#include <bits/stdc++.h>
using namespace std;

int main(){
  int N;
  int M;
  cin >> N >> M;
  vector<int> data(N, N);
  for(int i=0;i<N;i++){
    for(int j=0;j<N;j++){
      cin >> data.at(i).at(j);
    }
  }

  vector<vector<vector<int> dataset((N-M+1)*(N-M+1)-1, vector<int>(M, vector<int>(M)));
  for(int p=0;p<(N-M+1)*(N-M+1);p++){
    for(int q=0;q<N-M+1;q++){
      for(int k=0;k<M;k++){
        for(int l=0;l<M;l++){
          dataset.at(p).at(k+q).at(l) = data.at(k+q).at(l);
        }
      }  
    }
  }
  cout << count(set(dataset)) << endl;
}
