#include <bits/stdc.h++>
using namespace std;

int main(){
  int N;
  cin >> N;
  int x;
  int total(x){
    string y;
    y = string(x);
    for(int i =0;i<len(x);i++){
      int w=0;
      w += int(y[i]);
    }
    return w;
  }

  int n>=0;
  int A(n){
    if(n=0||n=1){
      return 1;
    }
    else{
      return A(n-1)+total(A(n-1));
    }
  }
  cout << A(N) << endl;
}
