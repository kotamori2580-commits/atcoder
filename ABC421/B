#include <bits/stdc++.h>
using namespace std;

int function(int x){
  string s;
  s = string(x);
  string text;
  for(int i=0;i<s.size();i++){
    text.push_back(s.at(s.size()-1-i));
  }
  return int(text);
}

int main(){
  int x,y;
  cin >> x >> y;
  int ans=0;
  int a=x;
  int freq_ans=y;
  for(int i=0;i<8;i++){
    if(i=0){
      ans = function(a+freq_ans);
      freq_ans = a;    
    }
    else{
      a = ans;
      ans = function(a+freq_ans);
      freq_ans = a;      
    }
  }
}
