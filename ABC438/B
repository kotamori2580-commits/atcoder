#include <bits/stdc++.h>
using namespace std;

int main(){
  int N,M;
  cin >> N >> M;
  string S,T;
  cin >> S >> T;
  long long ans =1000000000;
  for(int i=0;i<N-M+1;i++){
    string U;
    U = S.substr(i, M);
    long long new_ans=0;
    for(int j=0;j<M;j++){
      if(U[j]<T[j]){
        new_ans += U[j]+10-T[j];
      }
      else{
        new_ans += U[j]-T[j];
      }
    }
    ans = min(ans, new_ans);
  }
  cout << ans << "\n";
}
