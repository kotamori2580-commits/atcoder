#include <bits/stdc++.h>
using namespace std;

int main(){
  int Q;
  cin >> Q;
  vector<int> vec;
  int ans =0;
  for(int i=0;i<Q;i++){
    string q;
    cin >> q;
    if(q[0]=='1'){
      vec.push_back(q[2]-'0');
    }
    else{
      ans = *min_element(vec.begin(), vec.end());
      cout << ans << "\n";
    }
  }
}
