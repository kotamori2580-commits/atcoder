#include <bits/stdc++.h>
using namespace std;

int main(){
  int N;
  int M;
  cin >> N >> M;
  vector<int> a;
  for (int i=0;i<N;i++){
    cin >> a.at(i);
  }
  string flag = "NO";
  
  int sum=0;
  for (int k=0;k<N;k++){
    sum += a.at(k);
  }
  for (int j=0;j<N;j++){
    if (sum-a.at(j) == M ){
      flag = "YES";
    }
  }
  cout << flag << endl;
}
