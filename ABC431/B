# include <bits/stdc++.h>
using namespace std;

int main(){
  int X;
  cin >> X;
  int N;
  cin >> N;
  vector<int> weight(N);
  for (int k=0;k<N;k++){
    cin >> weight.at(k);
  }
  int Q;
  cin >> Q;
  vector<int> vec(Q);
  for(int j=0;j<Q;j++){
    cin >> vec.at(j);
  }
  
  vector<bool> attached(N, false);
  for(int i=0;i<Q;i++){
    if (attached.at(vec.at(i)-1) == false){
      X += weight.at(vec.at(i)-1);
      attached.at(vec.at(i)-1) = true;
    }
    else {
      X -= weight.at(vec.at(i)-1);
      attached.at(vec.at(i)-1) = false;
    }
    cout << X << endl;
  }
}
