#include <bits/stdc++.h>
using namespace std;

int main(){
  int h,w,n;
  cin >> h >> w >> n;
  
  vector<vector<int>> block(h, vector<int>(w));
  for(int i=0;i<h;i++){
    vector<int> a(w);
    for(int j=0;j<w;j++){
      cin >> a[j];
    }
    block[i] = a;
  }
 
  vector<int> count(h);
  for(int k=0;k<n;k++){
    int b;
    cin >> b;
    for(int i=0;i<h;i++){
      for(int j=0;j<w;j++){
        if(block[i][j]==b){
          count[i] ++;
        }
      }
    }
  }
  int ans;
  ans = max(count.begin(),count.end());
  cout << ans << "\n";
}
