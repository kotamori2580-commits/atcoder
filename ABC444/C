#include <bits/stdc++.h>
using namespace std;


int main(){
  long long N;
  cin >> N;
  vector<long long> A(N);
  long long max_A=0;
  long long sum_A=0;
  for(int i=0;i<N;i++){
    cin >> A[i];
    sum_A +=A[i];
    max_A = max(max_A, A[i]);
  }
  
  set<long long> candidate;
  candidate.insert(max_A);
  
  if((2*sum_A)%N==0){
    candidate.insert((2*sum_A)/N);
  }
  
  sort(A.begin(), A.end());
  vector<long long> valid_answer;
  for(long long L: candidate){
    if(L<max_A){
      continue;
    }
    int l=0;
    int r=N-1;
    bool flag = true;
    while(l<=r){
      if(A[r]==L){
        r--;
      }
      else if(l<r && A[l]+A[r]==L){
        l++;
        r--;
      }
      else{
        flag = false;
        break;
      }
    }
    if(flag){
      valid_answer.push_back(L);
    }
  }
  sort(valid_answer.begin(), valid_answer.end());
  for(int i=0;i<valid_answer.size();i++){
    cout << valid_answer[i] << (i == valid_answer.size()-1 ? "\n":" ");
  }
}
